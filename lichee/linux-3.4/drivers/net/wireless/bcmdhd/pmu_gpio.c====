#define AXP20_GPIO0_CTL		0x90
#define AXP20_GPIO1_CTL		0x92
#define AXP20_GPIO2_CTL		0x93
#define AXP20_GPIO3_CTL		0x95

#define AXP_GPIO012_STA		0x94



#define AXP_GPIO0_CFG                   (AXP20_GPIO0_CTL)//0x90
#define AXP_GPIO1_CFG                   (AXP20_GPIO1_CTL)//0x92

#define AXP_GPIO2_CFG                   (AXP20_GPIO0_CTL)//0x93
#define AXP_GPIO3_CFG                   (AXP20_GPIO1_CTL)//0x95